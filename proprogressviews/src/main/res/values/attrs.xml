<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DoubleArcProgress">
        <attr name="arcRadius" format="dimension"/>
        <attr name="colorofArc" format="color"/>
    </declare-styleable>

    <declare-styleable name="DottedArcProgress">
        <attr name="dots_radius" format="dimension"/>
        <attr name="dots_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="CircleLineProgress">
        <attr name="circle_radius" format="dimension"/>
        <attr name="circle_color" format="color"/>
        <attr name="line_radius" format="dimension"/>
        <attr name="line_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="CircleArcProgress">
        <attr name="circle_size" format="dimension"/>
        <attr name="color_circle" format="color"/>
        <attr name="arc_radius" format="dimension"/>
        <attr name="arc_color" format="color"/>
    </declare-styleable>

</resources>